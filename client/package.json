{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/auth/google": {
      "target": "http://localhost:5000"
    },
    "/api/*": {
      "target": "http://localhost:5000"
    }
  },
  "dependencies": {
    "@cszatma/react-scripts": "1.2.1",
    "axios": "^0.18.0",
    "materialize-css": "^0.100.2",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-stripe-checkout": "^2.6.3",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-enzyme": "^6.0.0",
    "lint-staged": "^7.0.4",
    "redux-mock-store": "^1.5.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "flow": "flow",
    "flow-typed-install": "flow-typed install -s -i dev",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"src/**/*.js\"",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "prettier": {
    "parser": "flow",
    "trailingComma": "all",
    "singleQuote": true,
    "tabWidth": 4
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
